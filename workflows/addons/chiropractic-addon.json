{
  "name": "Chiropractic Specialization Add-on",
  "nodes": [
    {
      "parameters": {
        "operation": "analyzeChiropracticInquiry",
        "inquiryData": "={{ $json.body }}",
        "specialization": "chiropractic",
        "triggers": {
          "emergency": ["severe_pain", "injury", "accident"],
          "routine": ["adjustment", "maintenance", "wellness"],
          "specialized": ["sports", "prenatal", "pediatric"]
        }
      },
      "name": "Chiropractic Analysis Agent",
      "type": "autopilotcx-nodes.specializedAgent",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "operation": "routeChiropracticInquiry",
        "analysisResult": "={{ $json.analysis }}",
        "routingRules": {
          "emergency": "immediate_human",
          "routine": "scheduler",
          "specialized": "specialist_coordinator"
        }
      },
      "name": "Chiropractic Routing Agent",
      "type": "autopilotcx-nodes.specializedAgent",
      "typeVersion": 1,
      "position": [450, 300]
    },
    {
      "parameters": {
        "operation": "scheduleChiropracticVisit",
        "clientId": "={{ $json.body.clientId }}",
        "visitType": "={{ $json.analysis.visitType }}",
        "duration": "={{ $json.analysis.estimatedDuration }}",
        "preferences": {
          "location": "={{ $json.body.preferredLocation }}",
          "time": "={{ $json.body.preferredTime }}",
          "chiropractor": "={{ $json.body.preferredChiropractor }}"
        }
      },
      "name": "Chiropractic Scheduler",
      "type": "autopilotcx-nodes.specializedAgent",
      "typeVersion": 1,
      "position": [650, 300]
    },
    {
      "parameters": {
        "operation": "prepareChiropracticVisit",
        "visitData": "={{ $json.visit }}",
        "requirements": {
          "forms": ["health_history", "injury_assessment"],
          "documents": ["insurance", "referral"],
          "preparation": ["comfortable_clothing", "previous_records"]
        }
      },
      "name": "Chiropractic Prep Agent",
      "type": "autopilotcx-nodes.specializedAgent",
      "typeVersion": 1,
      "position": [850, 300]
    },
    {
      "parameters": {
        "operation": "sendChiropracticInstructions",
        "clientId": "={{ $json.body.clientId }}",
        "instructionType": "pre_visit",
        "channels": ["email", "sms"],
        "content": {
          "preparation": "={{ $json.preparation.instructions }}",
          "what_to_bring": "={{ $json.preparation.required_items }}",
          "location_info": "={{ $json.visit.location_details }}",
          "post_adjustment_care": "={{ $json.preparation.post_care_instructions }}"
        }
      },
      "name": "Chiropractic Instruction Agent",
      "type": "autopilotcx-nodes.specializedAgent",
      "typeVersion": 1,
      "position": [1050, 300]
    },
    {
      "parameters": {
        "operation": "trackChiropracticMetrics",
        "practiceId": "={{ $json.body.practiceId }}",
        "metrics": [
          "visit_types",
          "emergency_cases",
          "specialist_referrals",
          "patient_satisfaction",
          "treatment_plans",
          "follow_up_rates"
        ]
      },
      "name": "Chiropractic Analytics Agent",
      "type": "autopilotcx-nodes.specializedAgent",
      "typeVersion": 1,
      "position": [1250, 300]
    }
  ],
  "connections": {
    "Chiropractic Analysis Agent": {
      "main": [[{"node": "Chiropractic Routing Agent", "type": "main", "index": 0}]]
    },
    "Chiropractic Routing Agent": {
      "main": [[{"node": "Chiropractic Scheduler", "type": "main", "index": 0}]]
    },
    "Chiropractic Scheduler": {
      "main": [[{"node": "Chiropractic Prep Agent", "type": "main", "index": 0}]]
    },
    "Chiropractic Prep Agent": {
      "main": [[{"node": "Chiropractic Instruction Agent", "type": "main", "index": 0}]]
    },
    "Chiropractic Instruction Agent": {
      "main": [[{"node": "Chiropractic Analytics Agent", "type": "main", "index": 0}]]
    }
  },
  "settings": {
    "executionOrder": "v1"
  },
  "metadata": {
    "specialization": "chiropractic",
    "version": "1.0.0",
    "compatibleWith": ["demo", "full"],
    "integrationPoints": {
      "demo": ["Demo Intake Webhook", "Maestro Agent"],
      "full": ["Lead Intake Webhook", "Maestro Agent"]
    }
  }
} 